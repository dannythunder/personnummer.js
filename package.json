{
    "name": "personnummer.js",
    "version": "1.1.6",
    "description": "JavaScript script to validate and parse Swedish personal identity numbers and corporate identity numbers. Written in TypeScript and includes TypeScript types.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "directories": {
        "test": "test"
    },
    "scripts": {
        "lint": "node_modules/typescript/bin/tsc --noEmit && node_modules/eslint/bin/eslint.js 'src/*/**/*.ts' --fix && echo '✔️ nice'",
        "prebuild": "npm run lint -s",
        "build": "node_modules/typescript/bin/tsc",
        "test": "mocha -r node_modules/ts-node/register/ test/**/*.test.ts",
        "babel-build": "babel lib -d dontpush",
        "prepare": "npm run test && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kelszo/personnummer.js.git"
    },
    "keywords": [
        "typescript",
        "ts",
        "personnummer",
        "validate",
        "validering",
        "validation",
        "swedish",
        "parsing",
        "parse",
        "personal",
        "number",
        "identity",
        "social",
        "security",
        "national",
        "organisations",
        "organisationsnummer",
        "luhn"
    ],
    "author": {
        "name": "Kelvin Szolnoky",
        "email": "kszolnoky@protonmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kelszo/personnummer.js/issues"
    },
    "homepage": "https://github.com/kelszo/personnummer.js/blob/master/README.md",
    "devDependencies": {
        "@babel/cli": "^7.6.4",
        "@babel/core": "^7.6.4",
        "@types/mocha": "^7.0.2",
        "@typescript-eslint/eslint-plugin": "^2.25.0",
        "@typescript-eslint/parser": "^2.25.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-node": "^11.0.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-security": "^1.4.0",
        "mocha": "^6.1.4",
        "prettier": "^2.0.5",
        "ts-node": "^8.9.1",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^3.8.3"
    },
    "files": [
        "lib"
    ]
}
